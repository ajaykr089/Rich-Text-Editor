{
  "name": "@editora/icons",
  "version": "0.1.2",
  "description": "Framework-agnostic SVG icon library for the Editora UI ecosystem with outline, solid, and duotone variants",
  "homepage": "https://editora-free.netlify.app/",
  "bugs": {
    "url": "https://github.com/ajaykr089/Editora/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajaykr089/Editora.git",
    "directory": "packages/icons"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "node ../../node_modules/vite/bin/vite.js build && npm run build:tsc",
    "build:tsc": "node ../../node_modules/typescript/bin/tsc -p . --emitDeclarationOnly --noEmit false --outDir dist",
    "dev": "node ../../node_modules/vite/bin/vite.js build --watch",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "rimraf": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "editora",
    "editora ui ecosystem",
    "icons",
    "svg",
    "ui-icons",
    "design-system",
    "theming",
    "icon-library",
    "framework-agnostic",
    "typescript",
    "admin-ui",
    "outline-icons",
    "duotone-icons"
  ],
  "author": "Ajay Kumar <ajaykr089@gmail.com>",
  "license": "MIT",
  "sideEffects": false
}

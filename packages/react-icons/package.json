{
  "name": "@editora/react-icons",
  "version": "0.1.2",
  "description": "React SVG icon components and theming context for the Editora UI ecosystem",
  "homepage": "https://editora-free.netlify.app/",
  "bugs": {
    "url": "https://github.com/ajaykr089/Editora/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajaykr089/Editora.git",
    "directory": "packages/react-icons"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "npm --prefix ../icons run build:tsc && node ../../node_modules/vite/bin/vite.js build && npm run build:tsc",
    "build:tsc": "npm --prefix ../icons run build:tsc && node ../../node_modules/typescript/bin/tsc -p . --emitDeclarationOnly --noEmit false --outDir dist",
    "dev": "node ../../node_modules/vite/bin/vite.js build --watch",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18 || ^19"
  },
  "dependencies": {
    "@editora/icons": "^0.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  },
  "keywords": [
    "editora",
    "editora ui ecosystem",
    "icons",
    "react",
    "svg",
    "react-icons",
    "design-system",
    "icon-library",
    "theming"
  ],
  "author": "Ajay Kumar <ajaykr089@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
